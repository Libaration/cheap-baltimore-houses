{
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.4",
    "turbo": "^1.6.2"
  },
  "scripts": {
    "start": "turbo run start",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "dbbackup": "cp apps/cheap-baltimore-houses/.tmp/data.db $HOME/Desktop/db",
    "dev": "turbo run dev --parallel --no-cache",
    "clean": "rm -rf apps/cheap-baltimore-houses/.cache && rm -rf apps/cheap-baltimore-houses/build && yarn cache clean && yarn && yarn build --no-cache",
    "postinstall": "patch-package",
    "client:test": "yarn workspace client test"
  },
  "dependencies": {
    "git": "^0.1.5",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0"
  },
  "packageManager": "yarn@3.2.4"
}
