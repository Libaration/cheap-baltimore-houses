{
  "kind": "collectionType",
  "collectionName": "homes",
  "info": {
    "singularName": "home",
    "pluralName": "homes",
    "displayName": "Home",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "description": {
      "type": "richtext"
    },
    "street": {
      "type": "string",
      "required": true
    },
    "street_2": {
      "type": "string"
    },
    "zip": {
      "type": "integer",
      "required": true
    },
    "state": {
      "type": "string",
      "default": "MD",
      "required": true
    },
    "additional_images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "price": {
      "type": "integer",
      "required": false
    },
    "bidders": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "bids"
    },
    "cover_image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "city": {
      "type": "string",
      "required": true,
      "default": "Baltimore"
    }
  }
}
